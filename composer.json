{
    "name": "jaanbv/smsbox-php-api",
    "description": "This library connects to the SmsBox api using php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jaan Colman",
            "email": "support@jaan.be"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-simplexml": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "myclabs/php-enum": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
